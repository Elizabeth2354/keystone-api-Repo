#%RAML 1.0 DataType

type: object
properties:
  countryCd:   
    type: string
    required: false
    pattern: "[0-9]{3}"
  areaCd:   
    type: string
    required: false
    pattern: "[0-9]{3}"
  phoneNbr:   
    type: string
    required: true
    maxLength: 7
  extension:   
    type: string
    required: false
    pattern: "[0-9]*" 